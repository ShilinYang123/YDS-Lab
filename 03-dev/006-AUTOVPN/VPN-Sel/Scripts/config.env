# AUTOVPN 配置文件 - 优化版
# 所有参数在此统一管理，修改后会自动同步到所有脚本和文档

# WebSocket隧道基本配置
PREFIX=xYz123AbC        # WebSocket路径前缀，用于认证
SERVER_IP=192.210.206.52  # 服务器IP地址
SERVER_PORT=443          # 服务器端口
SERVER_RESTRICT_PORT=8443 # 服务器限制转发端口
WSTUNNEL_PORT=1081        # WebSocket隧道端口

# 网络优化参数（新增）
MTU_SIZE=1420
KEEPALIVE_INTERVAL=60
CONNECT_TIMEOUT=10       # 连接超时时间（秒）
READ_TIMEOUT=30          # 读取超时时间（秒）
TCP_NODELAY=true         # 禁用Nagle算法，减少延迟
SOCKET_BUFFER_SIZE=65536 # Socket缓冲区大小

# DNS优化配置（新增）
PRIMARY_DNS=8.8.8.8      # Google DNS
SECONDARY_DNS=1.1.1.1    # Cloudflare DNS
DNS_OVER_HTTPS=true      # 启用DoH避免DNS污染
DNS_TIMEOUT=5            # DNS查询超时（秒）

# 代理服务配置
USER=youruser            # SOCKS5/HTTP代理用户名
PASS=yourpass            # SOCKS5/HTTP代理密码

# 服务器连接配置
SERVER_USER=root         # 服务器SSH用户名
SERVER_PASSWORD=Aa123456 # 服务器SSH密码
SOCKS5_PORT=1082         # SOCKS5代理端口
HTTP_PORT=8081           # HTTP代理端口

# WireGuard配置
WG_INTERFACE=PC1         # WireGuard接口名称
WG_LOCAL_IP=10.9.0.2     # 本机在WireGuard网络中的IP
WG_CONF_PATH=S:\YDS-Lab\03-dev\006-AUTOVPN\VPN-Sel\config\wireguard\client\PC1.conf # WireGuard配置文件路径

# 路径配置
SCRIPTS_DIR=S:\YDS-Lab\03-dev\006-AUTOVPN\VPN-Sel\Scripts
WSTUNNEL_DIR=S:\YDS-Lab\03-dev\006-AUTOVPN\VPN-Sel\Scripts
LOGS_DIR=S:\YDS-Lab\03-dev\006-AUTOVPN\VPN-Sel\logs
PAC_DIR=S:\YDS-Lab\03-dev\006-AUTOVPN\VPN-Sel\PAC

# 文件路径
DOMAIN_LIST_FILE=S:\YDS-Lab\03-dev\006-AUTOVPN\VPN-Sel\routes\需要获取IP的域名列表.txt
FAILED_DOMAINS_FILE=S:\YDS-Lab\03-dev\006-AUTOVPN\VPN-Sel\routes\解析失败域名列表.txt
IP_OUTPUT_FILE=S:\YDS-Lab\03-dev\006-AUTOVPN\VPN-Sel\routes\常用境外IP.txt

# 调试配置
DEBUG_MODE=false         # 是否启用调试模式
LOG_LEVEL=info           # 日志级别(debug, info, warning, error)

# 性能监控配置（新增）
ENABLE_SPEED_TEST=true   # 启用速度测试
SPEED_TEST_INTERVAL=300  # 速度测试间隔（秒）
LATENCY_THRESHOLD=200    # 延迟阈值（毫秒）
SPEED_THRESHOLD=10       # 速度阈值（Mbps）

# IPv6功能开关配置（新增）
IPv6_ENABLE=false
IPv6_PREFER=false      # true: 优先使用IPv6, false: 优先使用IPv4
IPv6_FALLBACK=true     # true: IPv6失败时回退到IPv4, false: 直接失败

# IPv6监听地址配置（当IPv6_ENABLE=false
IPv6_LISTEN_ADDR=[::1]   # IPv6本地监听地址
IPv6_PROXY_ADDR=[::]     # IPv6代理监听地址（所有IPv6地址）

# 服务器IPv6配置（可选）
SERVER_IPV6=            # 服务器IPv6地址（如果有，留空则使用IPv4）